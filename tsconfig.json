{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",
        "sourceMap": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
      },
      "exclude": [
        "node_modules"
    ],

    "include": [
        "src/**/*"
    ]
}